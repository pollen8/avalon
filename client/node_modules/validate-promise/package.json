{
  "name": "validate-promise",
  "version": "2.0.9",
  "description": "Promised based validation library",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --config ./webpack.config.js",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "test": "istanbul cover _mocha --recursive -x '**/*.spec.js' -x '**/config/**' -x '**/node_modules/**' -g './{,!(node_modules)/**/}*.spec.js'"
  },
  "keywords": [
    "validate"
  ],
  "author": "Rob Clayburn",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "eslint-plugin-flowtype": "^2.29.2",
    "flow-bin": "^0.37.4",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "webpack": "^1.14.0"
  },
  "babel": {
    "plugins": [
      "transform-flow-strip-types"
    ],
    "presets": [
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cognitivelogic/validate-promise.git"
  },
  "dependencies": {
    "install": "^0.8.8",
    "is-email": "^1.0.0",
    "npm": "^4.4.4"
  }
}
